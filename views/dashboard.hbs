<h3>Welcome {{name}}</h3>
<p>RSS URL:  <a href={{rssUrl}}>{{rssUrl}}</a></p>
<br>
<a href="/handles/" class="btn btn-secondary">Edit Tracked Handles</a>
<br>
<h4>Posts</h4>
{{#if posts}}
  <div>
  {{#each posts}}
      <div>
        <a href="/posts/{{this._id}}">{{this.title}}</a>
      </div>
  {{/each}}
  </div>
{{else}}
  <p>No post summaries saved in database</p>
{{/if}}
<br>
<a href="/users/logout" class="btn btn-secondary">Logout</a>
